{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": 0
    }
}
